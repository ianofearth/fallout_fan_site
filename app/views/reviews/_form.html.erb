<%= render 'layouts/errors', :object => @review %>

<%= form_for [@game, @review] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :opinion %>
  <%= f.text_field :opinion %>
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <%= f.label :number %>
  <%= f.text_field :number %>
<%= f.submit %>
<% end %>
